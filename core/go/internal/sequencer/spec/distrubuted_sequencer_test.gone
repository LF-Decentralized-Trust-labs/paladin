package spec

import (
	"context"
	"testing"
)

func TestDistrubtedSequencer_SingleTransactionToCompletion(t *testing.T) {
	ctx := context.Background()

	builder1 := NewSequencerBuilderForTesting().
		Node("node1")

	builder2 := NewSequencerBuilderForTesting().
		ContractAddress(builder1.GetContractAddress()).
		Node("node2")

	sequencer1, _ := builder1.Build(ctx)
	sequencer2, _ := builder2.Build(ctx)

}
